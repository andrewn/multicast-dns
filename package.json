{
  "name": "multicast-dns",
  "version": "2.1.0",
  "description": "Low level multicast-dns implementation in pure javascript",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/browserify -v background.js -o dist/background.js",
    "dev": "./node_modules/.bin/watchify -v -d background.js -o dist/background.js",
    "test": "standard && tape test.js"
  },
  "bin": "cli.js",
  "dependencies": {
    "thunky": "^0.1.0"
  },
  "devDependencies": {
    "standard": "^3.3.2",
    "tape": "^3.0.3",
    "watchify": "~3.1.1",
    "browserify": "~9.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mafintosh/multicast-dns.git"
  },
  "author": "Mathias Buus (@mafintosh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mafintosh/multicast-dns/issues"
  },
  "homepage": "https://github.com/mafintosh/multicast-dns",
  "keywords": [
    "multicast",
    "dns",
    "mdns",
    "multicastdns",
    "dns-sd",
    "service",
    "discovery",
    "bonjour",
    "avahi"
  ]
